solution code

// User function Template for Java


class Solution {
    long maxDays(int arr[]) {
        // code here
        int max=arr[0];
        for(int i=0;i<arr.length;i++){
            if(max<arr[i]){
                max=arr[i];
            }
        }
        return max;
    }
}